{
    "name": "rootwork/phalcon-extras",
    "description": "Phalcon Extras",
    "license": "BSD-3-Clause",
    "keywords": ["rootwork", "phalcon", "library", "extras"],
    "homepage": "https://github.com/rootworkit/phalcon-extras",
    "authors": [
        {
            "name": "Mike Soule",
            "email": "mike@rootwork.it"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "rootwork/phpunit-helpers": "dev-master",
        "firebase/php-jwt": "^3.0",
        "maknz/slack": "dev-master"
    },
    "autoload": {
        "psr-4": { "Rootwork\\Phalcon\\": "src/Phalcon/" }
    },
    "replace": {
        "rootwork/phalcon-toolbox": "0.1.*"
    },
    "suggest": {
        "firebase/php-jwt": "Required for JWT sessions",
        "maknz/slack": "Required to use the Slack log adapter"
    }
}
